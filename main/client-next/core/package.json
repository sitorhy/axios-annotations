{
  "name": "core-next",
  "version": "3.0.0",
  "description": "HTTP client library uses Axios without Typescript.",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./plugins": {
      "import": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/es/plugins/index.js"
      },
      "require": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/plugins/index.js"
      }
    },
    "./plugins/*": {
      "import": {
        "types": "./dist/plugins/*.d.ts",
        "default": "./dist/es/plugins/*.js"
      },
      "require": {
        "types": "./dist/plugins/*.d.ts",
        "default": "./dist/plugins/*.js"
      }
    }
  },
  "scripts": {
    "build": "gulp build"
  },
  "keywords": [
    "axios",
    "axios-annotations",
    "axios-decorators"
  ],
  "author": "sitorhy",
  "license": "MIT",
  "dependencies": {
    "@swc/core": "^1.10.1",
    "gulp": "^5.0.1",
    "gulp-swc": "^2.2.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-filter": "^9.0.1",
    "gulp-rename": "^2.1.0",
    "typescript": "^5.9.3",
    "axios": "^1.13.3",
    "rimraf": "^6.1.2"
  }
}
